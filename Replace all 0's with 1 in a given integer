#include<stdio.h>
int main()
{
	int n,i,arr[10];
	scanf("%d",&n);
	while (n != 0)
    {
        arr[i] =n % 10;
        n = n/10;
        i++;
    }
    for(int j=i-1;j>=0;j--)
    {
    	
      if(arr[j]==0)
      {
      	 printf("%d",arr[j]+1);
	  }
      else
      {
      	 printf("%d",arr[j]);
	  }
        
     }
       
    return 0;
}
