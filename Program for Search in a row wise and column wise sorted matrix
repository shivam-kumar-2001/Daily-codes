#include <iterator>
#include<bits/stdc++.h>
#include <array>
#include<algorithm>
#include <list>
#include <iostream>
using namespace std; 
int main() 
{ 
     std::vector<vector<int> > v{ {10, 20, 30, 40},
                      {15, 25, 35, 45},
                      {27, 29, 37, 48},
                      {32, 33, 39, 50}};
    int x=29;
    for(int i=0;i<v.size();i++)
    {
    	for(int j=0;j<v[i].size();j++)
    	{
    		if(v[i][j]==x)
    		{
    			cout<<"("<<i<<","<<j<<")"<<endl;
    		}
    	}
    }
    return 0; 
}
