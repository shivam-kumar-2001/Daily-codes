#include<stdio.h>
int main()
{
	int i = 0,a,number,k=1,sum=0,n;
	scanf("%d",&number);
	n=number;
	while(number != 0)
    {
    	k=1;
        a = number % 10;
        number /= 10;
        printf("%d ",a);
        for(i=1;i<=a;i++)
        {
        	 k*=i;
        
		} printf("%d\n",k);
		 sum+=k;
        i++;
    } printf("%d\n",sum);
    if(sum==n)
    {
    	printf("strong number");
	}
	else
	{
		printf("NOT strong");
	}
	 return 0;
}
