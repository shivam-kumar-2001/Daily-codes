#include<stdio.h>
int main()
{
	char n,b;
	scanf("%c",&n);
	if((n>64 && n<91) || (n>96 && n<123))
     
	 {
	   for(int i=0;i<=25;i++)
       {
    	if((i+65)==n)
    	{
    		printf("upper case"); break;
		}
		if((i+97)==n)
		{
			printf("lower case"); break;
		}
      }
   }
   else
       printf("special character");
	return 0;
}
