#include<stdio.h>
#include<math.h>
int main()
{
	int i;
     int x[2],y[2],a[2],sum=0;
	printf("enter coodinate x and y\n");
	for(i=0;i<2;i++)
	{
		scanf("%d%d",&x[i],&y[i]);
	}
	for(i=0;i<1;i++)
	{
		a[i]=sqrt(pow(x[i+1]-x[i],2) + pow(y[i+1] - y[i],2));
		printf("%d\n",a[i]);
	}
	for(i=0;i<1;i++)
	{
		sum+= a[i];
	} printf("%d",sum);
	return 0;
}
