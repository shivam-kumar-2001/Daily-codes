#include <iterator>
#include<bits/stdc++.h>
#include <array>
#include<algorithm>
#include <list>
#include <iostream>
#include<vector>
using namespace std;
int main()
{
	std::vector<int> arr={1,1,4,2,1,3};
	std::vector<int> arr1;
	arr1=arr;
	sort(arr1.begin(),arr1.end());
	int x=0;
	for(int i=0;i<arr.size();i++)
	  { 
	  	if(arr[i]!=arr1[i])
	  	{
	  		x++;
	  	}
		cout<<arr1[i]<<" ";
	} cout<<"\n"<<x;
	return 0;
}
