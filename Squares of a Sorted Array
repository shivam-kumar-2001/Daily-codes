#include <iterator>
#include<bits/stdc++.h>
#include <array>
#include<algorithm>
#include <list>
#include <iostream>
#include<vector>
using namespace std;
int main()
{
	std::vector<int> A={-4,-1,0,3,10};
	for(int i=0;i<A.size();i++)
	{
		A[i]=A[i]*A[i];
	}
	 sort(A.begin(),A.end());
	  for(int i=0;i<A.size();i++)
	 {
	 	cout<<A[i]<<" ";
	 } 
	return 0;
}
