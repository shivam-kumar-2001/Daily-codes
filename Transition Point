#include <bits/stdc++.h>
using namespace std;

int main()
{
  int n1,n2,a,x;
  cin>>n1;
  for(int i=0;i<n1;i++)
  { x=-1;
    cin>>n2;
    for(int j=0;j<n2;j++)
    {
      cin>>a;
      if(a==1)
      {
        continue;
      }
      else
      {
        x++;
      }
    }
    if(x==n2-1)
    {
      cout<<-1<<endl;
    }
    else
    {
       cout<<x+1<<endl;
    }
   
    
  }
  
  return 0;
}
